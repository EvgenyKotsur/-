<Window x:Class="ZhekaKotsur.Password"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZhekaKotsur"
        mc:Ignorable="d"
        Title="Password" Height="450" Width="800">
    <Grid Margin="0,0,10,0">
        <TextBox x:Name="loginTextBox" HorizontalAlignment="Center" Margin="0,92,0,0" TextWrapping="Wrap" Text="логин" VerticalAlignment="Top" Width="144" Height="32"/>
        <TextBox x:Name="passwordBox" HorizontalAlignment="Center" Margin="0,146,0,0" TextWrapping="Wrap" Text="пароль" VerticalAlignment="Top" Width="144" Height="32"/>
        <Button Content="войти" HorizontalAlignment="Center" VerticalAlignment="Center" Height="34" Width="100" Click="LoginButton_Click"/>
    </Grid>
</Window>
